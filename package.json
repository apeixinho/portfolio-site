  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "npm run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "npm run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "npm run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "> .01%",
    "not dead",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 1 Edge versions",
    "last 1 version",
    "Firefox ESR",
    "not ie <= 11"
  ],
